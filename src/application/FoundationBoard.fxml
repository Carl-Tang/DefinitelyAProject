<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FoundationBoardController">
   <children>
      <HBox prefHeight="50.0">
         <children>
            <Button fx:id="_homeBtn" mnemonicParsing="false" onAction="#backToHome" prefHeight="50.0" prefWidth="100.0" text="&lt;-Home" />
         </children>
      </HBox>
      <HBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="550.0" prefWidth="1000.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox alignment="CENTER" prefWidth="800.0" HBox.hgrow="ALWAYS">
               <children>
                  <HBox fx:id="_mainPane" alignment="CENTER" prefWidth="800.0" VBox.vgrow="ALWAYS" />
                  <HBox alignment="BOTTOM_LEFT" prefHeight="50.0">
                     <children>
                        <Button fx:id="_helpBtn" mnemonicParsing="false" onAction="#showHelp" prefHeight="50.0" prefWidth="50.0" text="Help" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <HBox fx:id="_statisticsPane" alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" />
         </children>
      </HBox>
   </children>
</VBox>
