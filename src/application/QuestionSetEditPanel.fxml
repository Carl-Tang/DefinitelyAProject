<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.QuestionSetEditPanelController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="800.0">
         <children>
            <ListView fx:id="listOfQuestions" prefHeight="350.0" prefWidth="700.0" />
            <HBox alignment="CENTER" prefHeight="150.0" prefWidth="800.0">
               <children>
                  <Button fx:id="addQuestionBtn" mnemonicParsing="false" onAction="#addNewQuestion" text="Add" />
                  <Region prefHeight="150.0" prefWidth="100.0" />
                  <Button fx:id="delteQuestionBtn" mnemonicParsing="false" onAction="#delteQuestion" text="Delete" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="confirmBtn" mnemonicParsing="false" onAction="#confirmAndExit" text="Confirm" />
                  <Region prefHeight="100.0" prefWidth="50.0" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</GridPane>
