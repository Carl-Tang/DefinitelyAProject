<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="550.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PractiseChoosingSceneController">
   <children>
      <Label text="Practise Maori Pronunciation">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <HBox alignment="CENTER">
         <children>
            <TextField fx:id="_numberTextField" prefHeight="40.0" prefWidth="400.0" promptText="Enter a number from 1 to 99">
               <HBox.margin>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="_specificPractiseButton" alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#specificPractiseButtonClicked" prefHeight="35.0" prefWidth="150.0" text="Pratise This Number">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="_autoGenerateButton" mnemonicParsing="false" onAction="#autoGenerateButtonClicked" prefHeight="35.0" prefWidth="150.0" text="Auto Generate (1 - 99)">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
