<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.chart.AreaChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PersonalPanelController">
	<children>
		<HBox alignment="CENTER_LEFT" minHeight="-Infinity" prefHeight="50.0" style="-fx-background-color: #1976d2;">
			<children>
				<JFXButton onAction="#backToHome" prefHeight="50.0" prefWidth="150.0" text="Home" textFill="WHITE">
					<font>
						<Font name="System Bold" size="14.0" />
					</font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
				</JFXButton>
				<Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
				<Label text="Personal Dashboard" textFill="WHITE">
					<font>
						<Font name="System Bold" size="18.0" />
					</font>
				</Label>
				<Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
				<JFXButton onAction="#showLeaderBoard" prefHeight="50.0" prefWidth="150.0" text="Go To LeaderBoard" textFill="WHITE">
					<font>
						<Font name="System Bold" size="14.0" />
					</font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
				</JFXButton>
			</children>
		</HBox>
		<Region prefHeight="40.0" />
      <VBox alignment="CENTER">
         <children>
      		<Label text="Player's Name:">
      			<font>
      				<Font name="System Bold" size="16.0" />
      			</font>
      		</Label>
      		<Label fx:id="_name" text="&lt;Name&gt;">
      			<font>
      				<Font size="36.0" />
      			</font>
      		</Label>
      		<HBox alignment="CENTER">
      			<children>
      				<Label text="Latest Score: ">
      					<font>
      						<Font name="System Bold" size="16.0" />
      					</font>
      				</Label>
      				<Label fx:id="_latestScore" text="&lt;Score&gt;">
      					<font>
      						<Font size="24.0" />
      					</font>
      				</Label>
      			</children>
      		</HBox>
            <HBox alignment="CENTER">
               <children>
                  <Label text="Mode: ">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="_latestMode" text="&lt;Normal/Endless&gt;">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding>
      </VBox>
		<Region prefHeight="40.0" />
		<HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
			<children>
				<VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
					<children>
						<HBox alignment="CENTER">
							<VBox.margin>
								<Insets />
							</VBox.margin>
							<children>
								<Label text="Normal Mode">
									<font>
										<Font name="Candara" size="16.0" />
									</font>
								</Label>
							</children>
						</HBox>
						<HBox alignment="CENTER_LEFT" minHeight="150.0" VBox.vgrow="ALWAYS">
							<children>
								<VBox style="-fx-background-color: #fafafa;" HBox.hgrow="ALWAYS">
									<children>
										<HBox alignment="CENTER_LEFT" style="-fx-background-color: #f5f5f5;">
											<children>
												<Label text="Current Rank" textFill="#000000b8">
													<HBox.margin>
														<Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
													</HBox.margin>
												</Label>
											</children>
										</HBox>
										<HBox alignment="CENTER" VBox.vgrow="ALWAYS">
											<children>
												<Label fx:id="_normalModeRank" text="&lt;RANK&gt;">
													<font>
														<Font name="System Bold" size="36.0" />
													</font>
													<HBox.margin>
														<Insets bottom="10.0" />
													</HBox.margin>
												</Label>
											</children>
										</HBox>
									</children>
									<HBox.margin>
										<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
									</HBox.margin>
									<effect>
										<DropShadow blurType="GAUSSIAN" color="#00000098" height="25.0" radius="12.0" width="25.0" />
									</effect>
								</VBox>
								<VBox layoutX="10.0" layoutY="10.0" style="-fx-background-color: #fafafa;" HBox.hgrow="ALWAYS">
									<children>
										<HBox alignment="CENTER_LEFT" style="-fx-background-color: #f5f5f5;">
											<children>
												<Label text="Personal Highest" textFill="#000000b8">
													<HBox.margin>
														<Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
													</HBox.margin>
												</Label>
											</children>
										</HBox>
										<HBox alignment="CENTER" VBox.vgrow="ALWAYS">
											<children>
												<Label fx:id="_normalModeScore" text="&lt;SCORE&gt;">
													<font>
														<Font name="System Bold" size="36.0" />
													</font>
													<HBox.margin>
														<Insets bottom="10.0" />
													</HBox.margin>
												</Label>
											</children>
										</HBox>
									</children>
									<HBox.margin>
										<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
									</HBox.margin>
									<effect>
										<DropShadow blurType="GAUSSIAN" color="#00000099" height="25.0" radius="12.0" width="25.0" />
									</effect>
								</VBox>
							</children>
							<VBox.margin>
								<Insets left="40.0" right="40.0" />
							</VBox.margin>
						</HBox>
						<VBox style="-fx-background-color: #fafafa;">
							<VBox.margin>
								<Insets left="50.0" right="50.0" />
							</VBox.margin>
							<children>
								<HBox style="-fx-background-color: #f5f5f5;">
									<VBox.margin>
										<Insets />
									</VBox.margin>
									<children>
										<Label text="History">
											<HBox.margin>
												<Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
											</HBox.margin>
										</Label>
									</children>
								</HBox>
								<AreaChart fx:id="_normalModeChart" legendVisible="false" maxHeight="200.0">
									<xAxis>
										<CategoryAxis side="BOTTOM" />
									</xAxis>
									<yAxis>
										<NumberAxis side="LEFT" />
									</yAxis>
								</AreaChart>
							</children>
							<effect>
								<DropShadow blurType="GAUSSIAN" color="#00000099" height="25.0" radius="12.0" width="25.0" />
							</effect>
						</VBox>
						<Region />
					</children>
				</VBox>
				<VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
					<children>
						<HBox alignment="CENTER">
							<VBox.margin>
								<Insets />
							</VBox.margin>
							<children>
								<Label text="Endless Mode">
									<font>
										<Font name="Candara" size="16.0" />
									</font>
								</Label>
							</children>
						</HBox>
						<HBox minHeight="150.0" VBox.vgrow="ALWAYS">
							<children>
								<VBox style="-fx-background-color: #fafafa;" HBox.hgrow="ALWAYS">
									<children>
										<HBox alignment="CENTER_LEFT" style="-fx-background-color: #f5f5f5;">
											<children>
												<Label text="Current Rank" textFill="#000000b8">
													<HBox.margin>
														<Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
													</HBox.margin>
												</Label>
											</children>
										</HBox>
										<HBox alignment="CENTER" VBox.vgrow="ALWAYS">
											<children>
												<Label fx:id="_endlessModeRank" text="&lt;RANK&gt;">
													<font>
														<Font name="System Bold" size="36.0" />
													</font>
													<HBox.margin>
														<Insets bottom="10.0" />
													</HBox.margin>
												</Label>
											</children>
										</HBox>
									</children>
									<HBox.margin>
										<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
									</HBox.margin>
									<effect>
										<DropShadow blurType="GAUSSIAN" color="#00000099" height="25.0" radius="12.0" width="25.0" />
									</effect>
								</VBox>
								<VBox layoutX="10.0" layoutY="10.0" style="-fx-background-color: #fafafa;" HBox.hgrow="ALWAYS">
									<children>
										<HBox alignment="CENTER_LEFT" style="-fx-background-color: #f5f5f5;">
											<children>
												<Label text="Personal Highest" textFill="#000000b8">
													<HBox.margin>
														<Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
													</HBox.margin>
												</Label>
											</children>
										</HBox>
										<HBox alignment="CENTER" VBox.vgrow="ALWAYS">
											<children>
												<Label fx:id="_endlessModeScore" text="&lt;SCORE&gt;">
													<font>
														<Font name="System Bold" size="36.0" />
													</font>
													<HBox.margin>
														<Insets bottom="10.0" />
													</HBox.margin>
												</Label>
											</children>
										</HBox>
									</children>
									<HBox.margin>
										<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
									</HBox.margin>
									<effect>
										<DropShadow blurType="GAUSSIAN" color="#00000099" height="25.0" radius="12.0" width="25.0" />
									</effect>
								</VBox>
							</children>
							<VBox.margin>
								<Insets left="40.0" right="40.0" />
							</VBox.margin>
						</HBox>
						<VBox style="-fx-background-color: #fafafa;">
							<VBox.margin>
								<Insets left="50.0" right="50.0" />
							</VBox.margin>
							<children>
								<HBox style="-fx-background-color: #f5f5f5;">
									<VBox.margin>
										<Insets />
									</VBox.margin>
									<children>
										<Label text="History">
											<HBox.margin>
												<Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
											</HBox.margin>
										</Label>
									</children>
								</HBox>
								<AreaChart fx:id="_endlessModeChart" legendVisible="false" maxHeight="200.0">
									<xAxis>
										<CategoryAxis side="BOTTOM" />
									</xAxis>
									<yAxis>
										<NumberAxis side="LEFT" />
									</yAxis>
								</AreaChart>
							</children>
							<effect>
								<DropShadow blurType="GAUSSIAN" color="#00000099" height="25.0" radius="12.0" width="25.0" />
							</effect>
						</VBox>
					</children>
				</VBox>
			</children>
		</HBox>
      <HBox minHeight="-Infinity" prefHeight="50.0">
         <children>
            <JFXButton onAction="#showHelp" prefHeight="50.0" prefWidth="80.0" text="Help" underline="true">
               <font>
                  <Font size="14.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></JFXButton>
         </children>
      </HBox>
	</children>
</VBox>
